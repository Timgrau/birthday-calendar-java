/* This Programm should be calender were you can store the birthdays of your family or friends
   It should order them by the next upcoming birthday. 
   You can check upcomming birthdays by command
   #EDIT
*/

import java.util.Scanner;

public class birthdayCalendar {
    
    DateTimeFormatter informatter = DateTimeFormatter.ofPattern("d.M.yyy");
    String name;

    /* constructer */
    public birthdayCalendar(String birthday, String name) {  
	this.name = name;
	LocaleDateTime bday = LocaleDateTime.parse(birthday,informatter);
    }
    
    public static void main (String[] args) {
	birthdayCalender a = new birthdayCalender(11.07.1997, Timo);
    }
}
